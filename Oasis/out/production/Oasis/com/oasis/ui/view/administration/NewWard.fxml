<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="backgroundAnchorPane" prefHeight="700.0"
            prefWidth="1100.0"
            stylesheets="@../../../resources/styles/default/new_edit_window.css" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="com.oasis.controller.administration.NewWardController">
    <children>
        <GridPane layoutX="14.0" layoutY="14.0" prefHeight="388.0" prefWidth="479.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="142.0" minWidth="10.0" prefWidth="142.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="291.0" minWidth="10.0" prefWidth="252.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Name"/>
                <Label text="Description" GridPane.rowIndex="1"/>
                <Label text="Max patient count" GridPane.rowIndex="2"/>
                <TextField fx:id="nameTextField" maxWidth="250.0" prefWidth="250.0" GridPane.columnIndex="1"/>
                <TextArea fx:id="descriptionTextArea" maxHeight="150.0" maxWidth="250.0" prefHeight="150.0"
                          prefWidth="250.0" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="maxPatientCountTextField" maxWidth="100.0" prefWidth="100.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="2"/>
                <Label text="Gender acceptance" GridPane.rowIndex="3"/>
                <ComboBox fx:id="genderAcceptanceComboBox" prefHeight="25.0" prefWidth="150.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="3"/>
                <Label text="Supervisor" GridPane.rowIndex="4"/>
                <AnchorPane fx:id="supervisorAnchorPane" maxHeight="25.0" prefHeight="25.0" GridPane.columnIndex="1"
                            GridPane.rowIndex="4"/>
            </children>
        </GridPane>
        <Button fx:id="okButton" layoutX="986.0" layoutY="651.0" mnemonicParsing="false" onAction="#okButtonOnAction"
                prefHeight="35.0" prefWidth="100.0" text="OK" AnchorPane.bottomAnchor="0.0"
                AnchorPane.rightAnchor="0.0"/>
    </children>
</AnchorPane>
